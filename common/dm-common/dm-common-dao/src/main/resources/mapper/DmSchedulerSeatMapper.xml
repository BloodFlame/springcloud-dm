<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.dm.mapper.DmSchedulerSeatMapper">

    <select id="getDmSchedulerSeatById" resultType="cn.dm.pojo.DmSchedulerSeat">
        select
                id as id,
                x as x,
                y as y,
                areaLevel as areaLevel,
                scheduleId as scheduleId,
                orderNo as orderNo,
                userId as userId,
                status as status,
                sort as sort,
                createdTime as createdTime,
                updatedTime as updatedTime
        from dm_scheduler_seat
        <trim prefix="where" prefixOverrides="and | or">
            <if test="id != null">
                and id=#{id}
            </if>
        </trim>
    </select>

    <select id="getDmSchedulerSeatListByMap" resultType="cn.dm.pojo.DmSchedulerSeat" parameterType="java.util.Map">
