    <insert id="insertDmSchedulerSeat" parameterType="cn.dm.pojo.DmSchedulerSeat">
        insert into dm_scheduler_seat(
                        x,
                        y,
                        areaLevel,
                        scheduleId,
                        orderNo,
                        userId,
                        status,
                        sort,
                        createdTime,
                        updatedTime)
        values(
                     #{x},
                     #{y},
                     #{areaLevel},
                     #{scheduleId},
                     #{orderNo},
                     #{userId},
                     #{status},
                     #{sort},
                     #{createdTime},
                    #{updatedTime})
    </insert>

    <update id="updateDmSchedulerSeat" parameterType="cn.dm.pojo.DmSchedulerSeat">
        update dm_scheduler_seat
        <trim prefix="set" suffixOverrides="," suffix="where id=#{id}">
                        <if test="x != null and x!=''">
                            x=#{x},
                        </if>
                        <if test="y != null and y!=''">
                            y=#{y},
                        </if>
                        <if test="areaLevel != null and areaLevel!=''">
                            areaLevel=#{areaLevel},
                        </if>
                        <if test="scheduleId != null and scheduleId!=''">
                            scheduleId=#{scheduleId},
                        </if>
                        <if test="orderNo!=''">
                            orderNo=#{orderNo},
                        </if>
                        <if test="userId!=''">
                            userId=#{userId},
                        </if>
                        <if test="status != null and status!=''">
